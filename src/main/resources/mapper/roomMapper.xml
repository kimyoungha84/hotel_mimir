<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.sist.room.RoomMapper">
	<select id="searchAllRoom" resultType="kr.co.sist.room.RoomDTO">
		SELECT faq_num, faq_title, faq_content, faq_date
		FROM faq
		ORDER BY faq_date DESC
	</select>
	
</mapper>