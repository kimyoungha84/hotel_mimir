<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.sist.filepath.FilePathMapper">

	<select id="selectAllFilePath" resultType="kr.co.sist.filepath.FilePathDomain">
		SELECT *
		FROM FILE_PATH
		WHERE PROPERTY_ID LIKE 'room_%'
	</select>
	
	
	
</mapper>